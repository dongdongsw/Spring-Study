package com.sist.main2;

public class Member {
	private int no;
	private String name;
	private String address;
	
	public Member(int no, String name, String addrss) {
		super();
		this.no = no;
		this.name = name;
		this.address = address;
	}
	public Member() {
		
	}
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAddrss() {
		return address;
	}
	public void setAddrss(String addrss) {
		this.address = address;
	}
	
	public void init() {
		System.out.println("===== 사원정보 ======");
	}
	public void destory() {
		
		System.out.println("===== 사원정보 ======");	
	}
	
	public void print() {
		System.out.println("번호" + no);
		System.out.println("번호" + name);
		System.out.println("번호" + address);
	}
}

