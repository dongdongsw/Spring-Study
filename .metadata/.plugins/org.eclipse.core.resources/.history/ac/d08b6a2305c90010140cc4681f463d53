public class MainClass {

	public static void main(String[] args) {

		String[] xml= {
				"student-app.xml",
				"member-app.xml",
				"sawon-app.xml"
				};
		ApplicationContext app = new ClassPathXmlApplicationContext(xml);
		
		Member m = (Member)app.getBean("mem");
		
		
	}

}
